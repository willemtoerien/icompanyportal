{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "users-api": [
        "dist/users-api/users-api",
        "dist/users-api"
      ],
      "notifications-api": [
        "dist/notifications-api/notifications-api",
        "dist/notifications-api"
      ],
      "companies-api": [
        "dist/companies-api/companies-api",
        "dist/companies-api"
      ],
      "utils": [
        "dist/utils/utils",
        "dist/utils"
      ],
      "auth-utils": [
        "dist/auth-utils/auth-utils",
        "dist/auth-utils"
      ],
      "company-utils": [
        "dist/company-utils/company-utils",
        "dist/company-utils"
      ],
      "notification-ex": [
        "src/app/modules/notification-ex"
      ],
      "forms-ex": [
        "dist/forms-ex/forms-ex",
        "dist/forms-ex"
      ],
      "notification-utils": [
        "dist/notification-utils/notification-utils",
        "dist/notification-utils"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}